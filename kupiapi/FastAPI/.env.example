# Kupi Bridge (FastAPI) - Environment Variables
# Přidej si vlastní konfigurace, pokud bude potřeba

# Timezone pro datum a čas
TZ=Europe/Prague

# FastAPI konfigurace
# FASTAPI_HOST=0.0.0.0
# FASTAPI_PORT=8000
# FASTAPI_RELOAD=true

# Kupiapi konfigurace
# KUPIAPI_MAX_PAGES=0  # 0 = bez limitu
# KUPIAPI_TIMEOUT=30   # timeout v sekundách

# Logging
# LOG_LEVEL=INFO

# CORS nastavení (pro produkci omezte!)
# CORS_ORIGINS=["*"]